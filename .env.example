# AURA Configuration Template
# Copy this to .env and fill in your actual values

# Google Gemini API Key
# For single key:
GEMINI_API_KEY=your_gemini_api_key_here

# For multiple keys (to avoid rate limits), separate with commas:
# GEMINI_API_KEY=key1_here,key2_here,key3_here

# Optional: OpenRouter API Key (for future features)
# OPENROUTER_API_KEY=your_openrouter_key_here

# Voice Settings
VOICE_LANGUAGE=en-US
WAKE_WORD=aura

# User Preferences
USER_NAME=Sir
